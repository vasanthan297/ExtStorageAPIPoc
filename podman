const url = 'https://aa.bb.cc.com/';

// Remove the 'https://' part
const withoutProtocol = url.replace('https://', '');

// Find the index of the next '/'
const nextSlashIndex = withoutProtocol.indexOf('/');

// Extract the substring between '//' and '/'
const extractedString = withoutProtocol.substring(0, nextSlashIndex);

console.log('Extracted String:', extractedString);
